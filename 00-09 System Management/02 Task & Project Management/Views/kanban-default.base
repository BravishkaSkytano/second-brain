filters:
  and:
    - file.hasTag("task")
views:
  - type: tasknotesKanban
    name: Kanban Board
    order:
      - status
      - priority
      - due
      - scheduled
      - projects
      - contexts
      - file.tags
      - blockedBy
      - timeEstimate
      - recurrence
      - file.name
      - complete_instances
    options:
      columnWidth: 280
      hideEmptyColumns: false
