views:
  - type: cards
    name: Cards
    filters:
      and:
        - file.tags.contains("course/ART101")
        - file.name != "ART101"
    order:
      - file.name
      - file.tags
      - created
      - modified
    sort:
      - property: modified
        direction: DESC
    imageFit: ""
    image: note.feature
