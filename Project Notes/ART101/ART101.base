views:
  - type: cards
    name: Cards
    filters:
      and:
        - file.tags.contains("ART101")
        - '!file.name.startsWith("ART101")'
    order:
      - file.name
      - file.tags
      - created
      - modified
    sort:
      - property: modified
        direction: DESC
    imageFit: ""
    image: note.image
