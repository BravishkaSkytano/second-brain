{
	"name": "the-thought-archive",
	"version": "1.3.0",
	"description": "A Second Brain/blog created using Eleventy and WebC.",
	"type": "module",
	"scripts": {
		"build:css": "tailwindcss -i _includes/layouts/css/styles.css -o _site/css/styles.css --minify",
		"build:11ty": "npx @11ty/eleventy",
		"build": "npm run build:css && npm run build:11ty",
		"dev:css": "tailwindcss -i _includes/layouts/css/styles.css -o _site/css/styles.css --watch",
		"dev:11ty": "cross-env ELEVENTY_ENV=development npx @11ty/eleventy --serve --quiet",
		"dev:test": "npx @11ty/eleventy --dryrun",
		"start": "concurrently \"npm:dev:css\" \"npm:dev:11ty\"",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"clean": "rimraf _site .cache node_modules/.cache"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/BravishkaSkytano/second-brain.git"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/BravishkaSkytano/second-brain/issues"
	},
	"homepage": "https://github.com/BravishkaSkytano/second-brain#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"@11ty/eleventy-plugin-webc": "^0.12.0-beta.7",
		"@iconify/json": "^2.2.431",
		"@iconify/tailwind4": "^1.2.1",
		"@tailwindcss/cli": "^4.1.18",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"flyonui": "^2.4.1",
		"prettier": "^3.8.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rimraf": "^6.1.2",
		"tailwindcss": "^4.1.18"
	},
	"dependencies": {
		"@11ty/is-land": "^5.0.1"
	}
}
